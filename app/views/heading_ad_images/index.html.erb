<p id="notice"><%= notice %></p>

<h1>Heading Ad Images</h1>

<table>
  <thead>
    <tr>
      <th>Ad image</th>
      <th>X</th>
      <th>Y</th>
      <th>Width</th>
      <th>Height</th>
      <th>X in unit</th>
      <th>Y in unit</th>
      <th>Width in unit</th>
      <th>Height in unit</th>
      <th>Page heading</th>
      <th>Advertiser</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @heading_ad_images.each do |heading_ad_image| %>
      <tr>
        <td><%= heading_ad_image.ad_image %></td>
        <td><%= heading_ad_image.x %></td>
        <td><%= heading_ad_image.y %></td>
        <td><%= heading_ad_image.width %></td>
        <td><%= heading_ad_image.height %></td>
        <td><%= heading_ad_image.x_in_unit %></td>
        <td><%= heading_ad_image.y_in_unit %></td>
        <td><%= heading_ad_image.width_in_unit %></td>
        <td><%= heading_ad_image.height_in_unit %></td>
        <td><%= heading_ad_image.page_heading %></td>
        <td><%= heading_ad_image.advertiser %></td>
        <td><%= link_to 'Show', heading_ad_image %></td>
        <td><%= link_to 'Edit', edit_heading_ad_image_path(heading_ad_image) %></td>
        <td><%= link_to 'Destroy', heading_ad_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Heading Ad Image', new_heading_ad_image_path %>
