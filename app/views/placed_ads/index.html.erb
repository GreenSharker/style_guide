<p id="notice"><%= notice %></p>

<h1>Placed Ads</h1>

<table>
  <thead>
    <tr>
      <th>Ad type</th>
      <th>Column</th>
      <th>Row</th>
      <th>Page</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @placed_ads.each do |placed_ad| %>
      <tr>
        <td><%= placed_ad.ad_type %></td>
        <td><%= placed_ad.column %></td>
        <td><%= placed_ad.row %></td>
        <td><%= placed_ad.page_id %></td>
        <td><%= link_to 'Show', placed_ad %></td>
        <td><%= link_to 'Edit', edit_placed_ad_path(placed_ad) %></td>
        <td><%= link_to 'Destroy', placed_ad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Placed Ad', new_placed_ad_path %>
