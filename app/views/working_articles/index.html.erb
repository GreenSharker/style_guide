<p id="notice"><%= notice %></p>

<h1>Working Articles</h1>

<table>
  <thead>
    <tr>
      <th>Page Number</th>
      <th>Column</th>
      <th>Row</th>
      <th>Order</th>
      <th>Title</th>
      <th>Subtitle</th>
      <th>Reporter</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @working_articles.each do |working_article| %>
      <tr>
        <td><%= working_article.page.page_number %></td>
        <td><%= working_article.column %></td>
        <td><%= working_article.row %></td>
        <td><%= working_article.order %></td>
        <td><%= working_article.title %></td>
        <td><%= working_article.subtitle %></td>
        <td><%= working_article.reporter %></td>
        
        <td><%= link_to 'Edit', change_story_working_article_path(working_article) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
