<div class="view">
  <% reporter_images.each do |picture| %>
    <div class="col-md-2">
      <img style="object-fit:cover",  src="<%= picture.thumb_path %>" width="200px" height="200px"></img>
      <p align="center"><%= picture.title %></p>
      <%= link_to '사진 선택', select_reporter_image_working_article_path(@working_article, reporter_image: picture) %>
    </div>
  <% end %>
</div>