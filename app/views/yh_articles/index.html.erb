<h1>Yh Articles</h1>

<table class="responsive nowrap table table-hover" id="dttb-yh_articles" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="action">Action</th>
      <th data-data="service_type">Service type</th>
      <th data-data="content_id">Content</th>
      <th data-data="date">Date</th>
      <th data-data="time">Time</th>
      <th data-data="urgency">Urgency</th>
      <th data-data="category">Category</th>
      <th data-data="class_code">Class code</th>
      <th data-data="attriubute_code">Attriubute code</th>
      <th data-data="source">Source</th>
      <th data-data="credit">Credit</th>
      <th data-data="region">Region</th>
      <th data-data="title">Title</th>
      <th data-data="body">Body</th>
      <th data-data="writer">Writer</th>
      <th data-data="char_count">Char count</th>
      <th data-data="taken_by">Taken by</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px"></th>
    </tr>
  </thead>

</table>

<br>

