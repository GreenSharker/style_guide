<%= simple_form_for(@ad_box) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :column %>
    <%= f.input :row %>
    <%= f.input :ad_type %>
    <%= f.input :advertiser %>
    <%= f.association :page %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
