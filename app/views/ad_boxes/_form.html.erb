<%= simple_form_for(@ad_box) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :column, label: '단' %>
    <%= f.input :row, label: '열' %>
    <%= f.input :ad_type, label: '광고종류'%>
    <%= f.input :advertiser, label: '광고주' %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
