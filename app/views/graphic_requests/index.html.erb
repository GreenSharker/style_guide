<!-- p id="notice"><%= notice %></p-->
<div align="center">
  <h3>그래픽 의뢰</h3>
</div>

<table border= '1' align="center">
  <thead>
    <tr>
      <th>일시</th>
      <th>제목</th>
      <th>의뢰인</th>
      <th>작업자</th>
      <th>진행상황</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @graphic_requests.each do |graphic_request| %>
      <tr>
        <td><%= graphic_request.date %></td>
        <td><%= graphic_request.title %></td>
        <td><%= graphic_request.requester %></td>
        <td><%= graphic_request.person_in_charge %></td>
        <td><%= graphic_request.status %></td>
        <td><%= link_to '상세보기', graphic_request %></td>
        <td><%= link_to '수정', edit_graphic_request_path(graphic_request) %></td>
        <td><%= link_to '삭제', graphic_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<div align="center">
  <%= link_to '새로운 그래픽 의뢰', new_graphic_request_path %>
</div>
