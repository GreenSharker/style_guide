<%= simple_form_for(@graphic_request) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :date %>
    <%= f.association :user %>
    <%= f.input :designer %>
    <%= f.input :request %>
    <%= f.input :data %>
    <%= f.input :status %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
