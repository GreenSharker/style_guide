<%= simple_form_for(@graphic_request) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :date %>
    <%= f.input :title %>
    <%= f.input :requester %>
    <%= f.input :person_in_charge %>
    <%= f.input :status %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
