
<div align="center">
  <%= link_to "이전으로", yh_pictures_path, method: :get, class: "btn btn-md btn-success" %>
  <%= link_to "사진선택", taken_yh_picture_path, method: :get, class: "btn btn-md btn-primary" %>
</div>
<br>
<div class="view">
  <div class="col-md-1">
  </div>

  <div class="col-md-5">
   <img  src="<%= @yh_picture.preview_path %>" width="100%"></img>
  </div>
  <div class="col-md-1">
  </div>
  <div class="col-md-4">

    <h3>
      <strong>제목:</strong>
      <%= @yh_picture.title %>
    </h3>

    <p>
      <strong>설명:</strong>
      <%= @yh_picture.body %>
    </p>

    <p>
      <strong>출고자:</strong>
      <%= @yh_picture.taken_by %>
    </p>

  </div>  
  </div>  


